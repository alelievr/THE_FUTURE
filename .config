#imac groups: post group

e1r1p8 1
e1r1p9 1
e1r1p10 1
e1r1p11 1
e1r1p12 1
e1r1p13 1
e1r1p14 1

1 shaders/fractal/mandel-simple.glsl 			#program id 0
1 shaders/raytracing-raymarching/lot-of-balls.glsl	#program id 1

#render loop groupId Focus programId
RenderLoop 1
{
#Focus syntax: Focus programIndex TransitonIndex Sync [None/Linear millisecs]
	Focus 0 0 Sync None
	Wait 5 #animation 1 time in seconds
	Uniform1f iGlobalTime 0 Sync None
	Focus 1 0 Sync Linear 400 #switch to program 1 and run it with 400ms delay for each imacs
	Wait 5
}
